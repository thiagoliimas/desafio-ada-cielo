{
  "openapi": "3.0.0",
  "info": {
    "title": "Exemplo de API com Swagger",
    "version": "1.0.0",
    "description": "Esta é uma especificação de exemplo do Swagger para uma API."
  },
  "paths": {
    "/cliente/pessoa-juridica": {
      "post": {
        "summary": "Descrição da operação POST",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "cpf": "12345678901",
                "merchantCategoryCode": 1234,
                "name": "Novo Nome",
                "email": "novoemail@example.com",
                "cnpj": "12345678901234",
                "companyName": "Nova Empresa"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Sucesso",
            "content": {
              "application/json": {
                "example": {
                  "message": "Operação bem-sucedida"
                }
              }
            }
          },
          "400": {
            "description": "Erro de solicitação",
            "content": {
              "application/json": {
                "example": {
                  "message": "Erro na solicitação"
                }
              }
            }
          }
        }
      }
    }
  }
}
